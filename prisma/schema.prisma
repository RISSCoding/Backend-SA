generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Role {
  USER
  ADMIN
}

model Account {
  userID      Int     @id @default(autoincrement())
  name        String
  phone       String
  position    String
  email       String
  password    String
  role        Role    @default(USER)
}